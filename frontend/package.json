{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --max-warnings 0 src/",
    "eject": "react-scripts eject",
    "fmt": "prettier --write \"src/**/*.js\"",
    "check-fmt": "prettier --check \"src/**/*.js\""
  },
  "devDependencies": {
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/setupTests.js"
    ]
  }
}
